  "design_characteristics": {
    "typography": {
      "approach": "bold_and_direct",
      "font_families": {
        "primary": ["Inter", "SF Pro", "system-ui", "-apple-system", "sans-serif"],
        "secondary": ["JetBrains Mono", "SF Mono", "Monaco", "monospace"],
        "display": ["Inter Display", "SF Pro Display", "system-ui", "sans-serif"]
      },
      "font_weights": {
        "light": "300",
        "regular": "400",
        "medium": "500",
        "semibold": "600",
        "bold": "700",
        "extrabold": "800"
      },
      "font_sizes": {
        "xs": "0.75rem",
        "sm": "0.875rem",
        "base": "1rem",
        "lg": "1.125rem",
        "xl": "1.25rem",
        "2xl": "1.5rem",
        "3xl": "1.875rem",
        "4xl": "2.25rem",
        "5xl": "3rem"
      },
      "line_heights": {
        "tight": "1.2",
        "snug": "1.375",
        "normal": "1.5", 
        "relaxed": "1.75",
        "loose": "2"
      },
      "fluid_typography": {
        "enabled": true,
        "base_size": "clamp(1rem, 2.5vw, 1.125rem)",
        "scale_ratio": 1.25,
        "viewport_units": "vw",
        "min_viewport": "20rem",
        "max_viewport": "80rem"
      },
      "variable_fonts": {
        "support": true,
        "font_variation_settings": {
          "weight": "wght 100 900",
          "optical_size": "opsz 8 144"
        }
      }
    },
    "spacing": {
      "scale_type": "geometric",
      "base_unit": "0.25rem",
      "scale_ratio": 1.5,
      "values": {
        "0": "0",
        "px": "1px",
        "0.5": "0.125rem",
        "1": "0.25rem",
        "1.5": "0.375rem",
        "2": "0.5rem",
        "2.5": "0.625rem",
        "3": "0.75rem",
        "3.5": "0.875rem",
        "4": "1rem",
        "5": "1.25rem",
        "6": "1.5rem",
        "7": "1.75rem",
        "8": "2rem",
        "9": "2.25rem",
        "10": "2.5rem",
        "11": "2.75rem",
        "12": "3rem",
        "14": "3.5rem",
        "16": "4rem",
        "20": "5rem",
        "24": "6rem",
        "28": "7rem",
        "32": "8rem"
      },
      "semantic_spacing": {
        "component_padding": "var(--linear-spacing-4)",
        "section_margin": "var(--linear-spacing-8)",
        "page_padding": "var(--linear-spacing-6)",
        "element_gap": "var(--linear-spacing-2)"
      }
    },
    "shadows": {
      "style": "subtle_soft",
      "color_opacity": 0.1,
      "elevation_levels": {
        "none": "none",
        "xs": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
        "sm": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
        "base": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
        "md": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
        "lg": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)",
        "xl": "0 25px 50px -12px rgba(0, 0, 0, 0.25)",
        "2xl": "0 25px 50px -12px rgba(0, 0, 0, 0.25)",
        "inner": "inset 0 2px 4px 0 rgba(0, 0, 0, 0.06)"
      },
      "dark_mode_adjustments": {
        "color_opacity": 0.3,
        "blur_increase": "2px"
      }
    },
    "borders": {
      "widths": {
        "0": "0px",
        "default": "1px",
        "2": "2px",
        "4": "4px",
        "8": "8px"
      },
      "radius": {
        "none": "0px",
        "sm": "0.125rem",
        "default": "0.25rem",
        "md": "0.375rem",
        "lg": "0.5rem",
        "xl": "0.75rem",
        "2xl": "1rem",
        "3xl": "1.5rem",
        "full": "9999px"
      },
      "styles": {
        "solid": "solid",
        "dashed": "dashed",
        "dotted": "dotted",
        "double": "double",
        "none": "none"
      }
    },
    "gradients": {
      "style": "angular_complex",
      "examples": [
        {
          "name": "linear_signature",
          "colors": ["#08AEEA", "#2AF598", "#B5FFFC", "#FF5ACD", "#FFFFFF"],
          "stops": ["0%", "25%", "50%", "75%", "100%"],
          "type": "linear",
          "angle": "135deg"
        },
        {
          "name": "accent_gradient",
          "colors": ["var(--linear-color-accent)", "var(--linear-color-accent-light)"],
          "stops": ["0%", "100%"],
          "type": "linear",
          "angle": "90deg"
        },
        {
          "name": "subtle_background",
          "colors": ["var(--linear-color-background)", "var(--linear-color-surface)"],
          "stops": ["0%", "100%"],
          "type": "radial",
          "position": "center"
        }
      ]
    },
    "grid_system": {
      "enabled": true,
      "container": {
        "max_width": "1200px",
        "padding": "1rem",
        "margin": "0 auto"
      },
      "columns": 12,
      "gap": "1.5rem",
      "breakpoint_columns": {
        "sm": 4,
        "md": 8,
        "lg": 12,
        "xl": 12
      }
    },
    "z_index": {
      "base": 0,
      "dropdown": 1000,
      "sticky": 1020,
      "fixed": 1030,
      "modal_backdrop": 1040,
      "modal": 1050,
      "popover": 1060,
      "tooltip": 1070,
      "toast": 1080
    }
  },
  "component_states": {
    "description": "컴포넌트별 인터랙션 상태 정의",
    "button_states": {
      "default": {
        "background": "var(--linear-color-accent)",
        "color": "var(--linear-color-accent-contrast)",
        "border": "1px solid transparent",
        "transform": "scale(1)",
        "transition": "all 150ms ease-in-out"
      },
      "hover": {
        "background": "var(--linear-color-accent-hover)",
        "transform": "scale(1.02)",
        "shadow": "var(--linear-shadow-md)"
      },
      "active": {
        "background": "var(--linear-color-accent-active)",
        "transform": "scale(0.98)",
        "shadow": "var(--linear-shadow-sm)"
      },
      "focus": {
        "outline": "2px solid var(--linear-color-accent)",
        "outline_offset": "2px"
      },
      "disabled": {
        "background": "var(--linear-color-neutral)",
        "color": "var(--linear-color-neutral-contrast)",
        "opacity": "0.6",
        "cursor": "not-allowed",
        "transform": "none"
      }
    },
    "input_states": {
      "default": {
        "background": "var(--linear-color-surface)",
        "border": "1px solid var(--linear-color-border)",
        "color": "var(--linear-color-text)",
        "transition": "all 200ms ease-in-out"
      },
      "focus": {
        "border": "1px solid var(--linear-color-accent)",
        "box_shadow": "0 0 0 3px var(--linear-color-accent-alpha-20)",
        "outline": "none"
      },
      "error": {
        "border": "1px solid var(--linear-color-error)",
        "box_shadow": "0 0 0 3px var(--linear-color-error-alpha-20)"
      },
      "success": {
        "border": "1px solid var(--linear-color-success)",
        "box_shadow": "0 0 0 3px var(--linear-color-success-alpha-20)"
      }
    },
    "card_states": {
      "default": {
        "background": "var(--linear-color-surface)",
        "border": "1px solid var(--linear-color-border)",
        "shadow": "var(--linear-shadow-sm)",
        "transform": "translateY(0)",
        "transition": "all 300ms ease-out"
      },
      "hover": {
        "shadow": "var(--linear-shadow-lg)",
        "transform": "translateY(-2px)",
        "border": "1px solid var(--linear-color-border-hover)"
      },
      "active": {
        "shadow": "var(--linear-shadow-md)",
        "transform": "translateY(-1px)"
      }
    }
  },
  "ui_patterns": {
    "layout": {
      "sidebar": {
        "width": "240px",
        "collapsed_width": "64px",
        "collapsible": true,
        "style": "minimal_icons",
        "background": "var(--linear-color-surface-elevated)",
        "border": "1px solid var(--linear-color-border-subtle)"
      },
      "main_content": {
        "max_width": "1200px",
        "padding": "24px",
        "margin": "0 auto",
        "responsive": true,
        "container_queries": true
      },
      "navigation": {
        "style": "breadcrumb_linear",
        "hierarchy": "clear_visual",
        "sticky": true,
        "backdrop_blur": "8px"
      },
      "header": {
        "height": "64px",
        "background": "var(--linear-color-background-alpha-90)",
        "backdrop_filter": "blur(8px)",
        "border_bottom": "1px solid var(--linear-color-border-subtle)"
      }
    },
    "components": {
      "buttons": {
        "primary": {
          "background": "var(--linear-color-accent)",
          "color": "var(--linear-color-accent-contrast)",
          "border_radius": "6px",
          "padding": "8px 16px",
          "font_weight": "500",
          "min_height": "40px"
        },
        "secondary": {
          "background": "transparent",
          "border": "1px solid var(--linear-color-accent)",
          "color": "var(--linear-color-accent)",
          "border_radius": "6px",
          "padding": "8px 16px"
        },
        "ghost": {
          "background": "transparent",
          "color": "var(--linear-color-text-secondary)",
          "border": "none",
          "padding": "8px 16px"
        }
      },
      "cards": {
        "default": {
          "background": "var(--linear-color-surface-elevated)",
          "border": "1px solid var(--linear-color-border-subtle)",
          "border_radius": "8px",
          "padding": "16px",
          "shadow": "var(--linear-shadow-sm)"
        },
        "interactive": {
          "cursor": "pointer",
          "transition": "all 300ms ease-out"
        }
      },
      "inputs": {
        "default": {
          "background": "var(--linear-color-surface-input)",
          "border": "1px solid var(--linear-color-border-input)",
          "border_radius": "6px",
          "padding": "8px 12px",
          "min_height": "40px",
          "font_size": "14px"
        },
        "focus_ring": {
          "box_shadow": "0 0 0 3px var(--linear-color-accent-alpha-20)",
          "border_color": "var(--linear-color-accent)"
        }
      },
      "modals": {
        "backdrop": {
          "background": "rgba(0, 0, 0, 0.5)",
          "backdrop_filter": "blur(4px)"
        },
        "container": {
          "background": "var(--linear-color-surface)",
          "border_radius": "12px",
          "padding": "24px",
          "max_width": "500px",
          "shadow": "var(--linear-shadow-2xl)"
        }
      }
    }
  },
  "mobile_optimization": {
    "description": "모바일 디바이스 최적화 설정",
    "touch_targets": {
      "minimum_size": "44px",
      "recommended_size": "48px",
      "spacing": "8px"
    },
    "safe_areas": {
      "top": "env(safe-area-inset-top)",
      "right": "env(safe-area-inset-right)",
      "bottom": "env(safe-area-inset-bottom)",
      "left": "env(safe-area-inset-left)"
    },
    "viewport_units": {
      "dynamic_viewport_height": "100dvh",
      "small_viewport_height": "100svh",
      "large_viewport_height": "100lvh"
    },
    "font_scaling": {
      "respect_user_preferences": true,
      "minimum_font_size": "16px",
      "maximum_scale": "200%"
    },
    "gestures": {
      "swipe_threshold": "50px",
      "tap_delay": "300ms",
      "long_press_duration": "500ms"
    }
  },
  "accessibility": {
    "contrast_ratios": {
      "aa_normal": "4.5:1",
      "aa_large": "3:1", 
      "aaa_normal": "7:1",
      "aaa_large": "4.5:1"
    },
    "high_contrast_mode": {
      "available": true,
      "contrast_multiplier": 3.33,
      "automatic_detection": true,
      "media_query": "@media (prefers-contrast: high)"
    },
    "keyboard_navigation": {
      "support": "full_support",
      "focus_visible": true,
      "skip_links": true,
      "focus_trap": true
    },
    "screen_reader": {
      "compliance": "aria_compliant",
      "live_regions": true,
      "descriptive_labels": true,
      "semantic_html": true
    },
    "reduced_motion": {
      "media_query": "@media (prefers-reduced-motion: reduce)",
      "fallback_transitions": "opacity, color",
      "disable_parallax": true
    },
    "color_blind_support": {
      "deuteranopia_safe": true,
      "protanopia_safe": true,
      "tritanopia_safe": true,
      "pattern_alternatives": true
    }
  },
  "internationalization": {
    "description": "다국어 및 국제화 지원",
    "rtl_support": {
      "enabled": true,
