# ==========================================
# 엘더베리 프론트엔드 - 환경변수 설정 예제
# 실제 사용시 .env 파일로 복사하여 값 설정
# React + Vite 환경
# ==========================================

# === API 설정 ===
VITE_API_BASE_URL=http://localhost:8080/api
VITE_WS_URL=ws://localhost:8080/ws

# === 앱 기본 설정 ===
VITE_APP_TITLE=엘더베리 케어링크
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=development

# === 카카오 API 키 ===
# 주의: 프론트엔드에서는 JavaScript 키만 사용 권장
VITE_KAKAO_JAVASCRIPT_KEY=your-kakao-javascript-key-here
# REST API 키는 브라우저 노출용 (선택사항)
VITE_KAKAO_REST_API_KEY=your-kakao-rest-api-key-here

# === 공공데이터 API 키 ===
# 주의: 브라우저에 노출되므로 클라이언트 사이드 전용 키 사용
VITE_PUBLIC_DATA_API_KEY=your-public-data-api-key-here
VITE_PUBLIC_DATA_API_KEY_ENCODED=your-encoded-public-data-api-key-here

# === 외부 서비스 설정 ===
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here
VITE_ANALYTICS_ID=your-analytics-id-here

# === 개발 서버 설정 ===
VITE_HOST=0.0.0.0
VITE_PORT=5173

# === 디버깅 설정 ===
VITE_DEBUG_MODE=true
VITE_ENABLE_DEVTOOLS=true

# === 성능 모니터링 ===
VITE_ENABLE_PERFORMANCE_MONITOR=true
VITE_ENABLE_ERROR_BOUNDARY=true

# === 기능 플래그 ===
VITE_ENABLE_CHATBOT=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=false