{
  "design_system": {
    "name": "Linear Design System Enhanced",
    "version": "2025.2.0",
    "description": "Linear의 미니멀하고 전문적인 디자인 시스템을 기반으로 한 완전한 테마 구성 - 실무 최적화",
    "brand_identity": {
      "primary_color": "#5E6AD2",
      "description": "미묘하고 탈색된 블루 - 밝고 어두운 배경에 모두 적합",
      "wordmark_usage": "단색 사용 권장",
      "company_name": "Linear (대문자 L로 시작하는 단일 단어)",
      "brand_colors": {
        "primary": "#5E6AD2",
        "secondary": "#8B5CF6", 
        "accent": "#06B6D4",
        "neutral": "#64748B"
      }
    },
    "core_principles": [
      "minimalism", 
      "clarity", 
      "performance", 
      "accessibility",
      "linear_flow",
      "progressive_enhancement",
      "mobile_first"
    ]
  },
  "theme_generation_system": {
    "color_space": "LCH",
    "description": "인간의 눈에 가장 가까운 색공간으로, 일관되게 좋은 테마 생성 가능",
    "core_variables": {
      "base_color": {
        "type": "LCH",
        "description": "기본 배경색과 표면 색상의 베이스",
        "example": "[95.07617314910061, 2.1856276247566773, 265.9705972968138, 1]"
      },
      "accent_color": {
        "type": "LCH", 
        "description": "강조색상 - 버튼, 링크, 활성 요소",
        "example": "[43.717135811988086, 99.37386079300107, 307.12305463765506, 1]"
      },
      "contrast": {
        "type": "number",
        "range": "0-100",
        "description": "테마의 대비도 - 접근성을 위한 고대비 테마 지원",
        "default": 30,
        "high_contrast": 100
      }
    },
    "generated_tokens": {
      "surfaces": {
        "background": "자동 생성",
        "foreground": "자동 생성", 
        "panels": "자동 생성",
        "dialogs": "자동 생성",
        "modals": "자동 생성",
        "cards": "자동 생성",
        "tooltips": "자동 생성"
      },
      "text_colors": "자동 생성",
      "icon_colors": "자동 생성",
      "control_colors": "자동 생성",
      "state_colors": "자동 생성"
    },
    "color_algorithm": {
      "lightness_scale": [5, 10, 15, 20, 30, 40, 50, 60, 70, 80, 90, 95, 98],
      "chroma_adjustments": {
        "low_contrast": 0.7,
        "normal_contrast": 1.0,
        "high_contrast": 1.3
      },
      "hue_variations": {
        "analogous": [-30, -15, 0, 15, 30],
        "complementary": [180],
        "triadic": [120, 240]
      }
    }
  },
  "state_colors": {
    "description": "시맨틱 컬러 시스템 - 사용자 인터페이스 상태 표현",
    "semantic_colors": {
      "success": {
        "light": "[65, 85, 160, 1]",
        "dark": "[70, 80, 160, 1]",
        "description": "성공, 완료, 승인 상태",
        "usage": ["성공 메시지", "완료된 작업", "승인된 항목"]
      },
      "warning": {
        "light": "[70, 90, 70, 1]",
        "dark": "[75, 85, 70, 1]",
        "description": "주의, 경고 상태",
        "usage": ["경고 메시지", "주의 필요 항목", "임시 상태"]
      },
      "error": {
        "light": "[55, 90, 30, 1]",
        "dark": "[60, 85, 30, 1]",
        "description": "오류, 실패, 위험 상태",
        "usage": ["오류 메시지", "실패한 작업", "위험 요소"]
      },
      "info": {
        "light": "[60, 80, 250, 1]",
        "dark": "[65, 75, 250, 1]",
        "description": "정보, 안내 상태",
        "usage": ["정보 메시지", "도움말", "안내사항"]
      },
      "neutral": {
        "light": "[85, 5, 270, 1]",
        "dark": "[25, 8, 270, 1]",
        "description": "중립, 비활성 상태",
        "usage": ["비활성 요소", "플레이스홀더", "보조 정보"]
      }
    },
    "interaction_states": {
      "default": "base_color",
      "hover": "base_color + 5% lightness",
      "active": "base_color - 5% lightness",
      "focus": "accent_color + focus_ring",
      "disabled": "neutral + 50% opacity"
    }
  },
  "animations": {
    "description": "모던 웹 인터페이스를 위한 애니메이션 시스템",
    "durations": {
      "instant": "0ms",
      "fast": "150ms",
      "normal": "250ms",
      "slow": "400ms",
      "slower": "600ms"
    },
    "easing_functions": {
      "linear": "linear",
      "ease_in": "cubic-bezier(0.4, 0, 1, 1)",
      "ease_out": "cubic-bezier(0, 0, 0.2, 1)",
      "ease_in_out": "cubic-bezier(0.4, 0, 0.2, 1)",
      "bounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)",
      "spring": "cubic-bezier(0.175, 0.885, 0.32, 1.275)"
    },
    "micro_interactions": {
      "button_press": {
        "duration": "150ms",
        "easing": "ease_in_out",
        "transform": "scale(0.98)"
      },
      "card_hover": {
        "duration": "250ms",
        "easing": "ease_out",
        "transform": "translateY(-2px)",
        "shadow": "elevated"
      },
      "modal_enter": {
        "duration": "400ms",
        "easing": "spring",
        "opacity": "0 to 1",
        "transform": "scale(0.9) to scale(1)"
      }
    },
    "reduced_motion": {
      "enabled": true,
      "fallback_duration": "0ms",
      "respect_prefers_reduced_motion": true,
      "alternative_feedback": ["color_change", "opacity_change", "border_highlight"]
    }
  },
